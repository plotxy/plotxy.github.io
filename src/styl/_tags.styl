.tags
    margin-top rem(30px)
    a
        font-size rem(14px)
        color maincolor
        display inline-block
        border 1px solid maincolor
        border-radius rem(5px)
        padding rem(4px) rem(10px)
        margin-right rem(2px)
        margin-bottom rem(8px)
        text-decoration none
        &:hover
            color tagshover
            border 1px solid tagshover
